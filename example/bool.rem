fn is_prime(n) {
    if n < 2 then return false
    if n < 4 then return true

    let i = 2
    while i < n do {
        if n % i == 0 then return false
        let i = i + 1
    }
    true
}


fn main(argc, argv) {
    let result = is_prime({
        let value
        sscanf(argv[1], "%d", &value)
        value
    })

    printf("prime number? = [%s]\n", if result then "TRUE" else "FALSE")
}
